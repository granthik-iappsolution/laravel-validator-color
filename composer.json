{
    "name": "nikhilbhatia22/laravel-validator-color",
    "description": "Validate that a certain value is a valid CSS color string in Laravel 5, 6 and 7",
    "keywords": [
        "laravel",
        "validator",
        "color",
        "nikhilbhatia22"
    ],
    "homepage": "https://github.com/nikhilbhatia22/laravel-validator-color",
    "license": "MIT",
    "authors": [
        {
            "name": "Tyler Arbon",
            "email": "tylercd100@gmail.com"
        },
        {
            "name": "Nikhil Bhatia",
            "email": "hello@nikhilbhatia.in"
        }
    ],
    "autoload":{
        "psr-4":{
            "Nikhilbhatia22\\Validator\\Color\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nikhilbhatia22\\Validator\\Color\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "illuminate/validation": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "php": "^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^5.0"
    },
    "suggest": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nikhilbhatia22\\Validator\\Color\\ServiceProvider"
            ]
        }
    }
}
